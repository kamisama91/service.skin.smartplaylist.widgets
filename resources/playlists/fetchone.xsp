<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="{PLALIST_TYPE}">
    <name>fetch_{PLAYLIST_NAME}_{FILE_ID}</name>
    <match>all</match>
	<rule field="playlist" operator="is"><value>{PLAYLIST_NAME}</value></rule>
	<rule field="path" operator="is"><value>{FILE_PATH}</value></rule>
	<rule field="filename" operator="is"><value>{FILE_NAME}</value></rule>
	<limit>1</limit>
</smartplaylist>
